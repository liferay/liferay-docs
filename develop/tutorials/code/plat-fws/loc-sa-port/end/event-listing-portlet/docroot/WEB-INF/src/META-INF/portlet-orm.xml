<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.samples.portlet.eventlisting.model.impl.EventModelImpl">
		<attributes>
			<id name="eventId" />
			<basic name="name" />
			<basic name="description" />
			<basic name="date">
				<column name="DATE_" />
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="userId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="locationId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.samples.portlet.eventlisting.model.impl.LocationModelImpl">
		<attributes>
			<id name="locationId" />
			<basic name="name" />
			<basic name="description" />
			<basic name="streetAddress" />
			<basic name="city" />
			<basic name="stateOrProvince" />
			<basic name="country" />
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="userId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.samples.portlet.eventlisting.model.impl.EventImpl" name="Event">
		<table name="Event" />
		<attributes />
	</entity>
	<entity class="com.liferay.samples.portlet.eventlisting.model.impl.LocationImpl" name="Location">
		<table name="Location" />
		<attributes />
	</entity>
</entity-mappings>