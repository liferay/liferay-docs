<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.3+</version>
	</compatibility>
	<theme id="lunar-resort-ce" name="Lunar Resort CE">
		<template-extension>ftl</template-extension>
		<settings>
			<setting configurable="true" key="show-breadcrumbs" options="true,false" type="select" value="false" />
			<setting configurable="true" key="portlet-setup-show-borders-default" type="checkbox" value="true" />
			<setting configurable="true" key="show-page-title" options="true,false" type="select" value="true"/>
		</settings>
	</theme>
</look-and-feel>