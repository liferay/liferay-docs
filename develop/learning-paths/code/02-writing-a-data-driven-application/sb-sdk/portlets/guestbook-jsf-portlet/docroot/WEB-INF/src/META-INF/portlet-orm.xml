<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.docs.guestbook.model.impl.EntryModelImpl">
		<attributes>
			<id name="entryId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="name" />
			<basic name="email" />
			<basic name="message" />
			<basic name="guestbookId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.docs.guestbook.model.impl.GuestbookModelImpl">
		<attributes>
			<id name="guestbookId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="name" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.docs.guestbook.model.impl.EntryImpl" name="Entry">
		<table name="GB_Entry" />
		<attributes />
	</entity>
	<entity class="com.liferay.docs.guestbook.model.impl.GuestbookImpl" name="Guestbook">
		<table name="GB_Guestbook" />
		<attributes />
	</entity>
</entity-mappings>